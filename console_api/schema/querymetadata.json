      "additionalProperties": false,
      "properties": {
        "pageNo": {
          "type": "integer",
          "mininum": 0
        },
        "pageSize":{
          "type": "integer",
          "mininum": 0
        },
        "totalRows": {
          "type": "integer",
          "mininum": 0
        },
        "nextPageNo": {
          "type": "integer",
          "mininum": 0
        },
        "totalPages": {
          "type": "integer",
          "mininum": 0
        },
        "previousPageNo": {
          "type": "integer",
          "mininum": 0
        },
        "data": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["metaId", "key", "keyType", "dataSetName", "dataSetId", "dataServerId", "dsName", "isCache", "isDelete"],
            "additionalProperties": false,
            "properties": {
              "metaId": {
                "type": "string"
              },
              "key": {
                "type": "string"
              },
              "keyType": {
                "type": "string",
                "enum": ["double","int","string", "int32", "int64"]
              },
              "dataSetName": {
                "type": "string"
              },
              "dataSetId":{
                "type": "integer"
              },
              "dataServerId": {
                "type": "integer"
              },
              "dsName": {
                "type": "string"
              },
              "isCache": {
                "type": "integer",
                "enum": [0, 1]
              },
              "isDelete": {
                "type": "integer",
                "enum": [0, 1]
              }
            }
          }
