        "previousPageNo": {
          "type": "integer",
          "mininum": 0
        },
        "data": {
          "type": "arrary",
          "items": {
            "type": "object",
            "required": ["id", "name", "dataServerId", "isDelete"],
            "additionalProperties": false,
            "properties": {
              "id": {
                "type": "integer",
