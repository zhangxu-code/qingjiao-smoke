{
  "$schema": "GET/api/api-tm/v1/task",
  "title": "get task response",
  "description": "get task response",
  "type": "object",
  "required": [
    "code",
    "data",
    "msg",
    "subCode"
  ],
  "additionalProperties": false,
  "properties": {
    "code": {
      "type": "integer",
      "enum": [0,1]
    },
    "msg": {
      "type": "string"
    },
    "subCode": {
      "type": ["null", "string"]
    },
    "data": {
      "type": "object",
      "required": ["pageNo", "pageSize", "totalRows", "nextPageNo", "totalPages", "previousPageNo", "data"],
      "additionalProperties": false,
      "properties": {
        "pageNo": {
          "type": "integer",
          "mininum": 0
        },
        "pageSize":{
          "type": "integer",
          "mininum": 0
        },
        "totalRows": {
          "type": "integer",
          "mininum": 0
        },
        "nextPageNo": {
          "type": "integer",
          "mininum": 0
        },
        "totalPages": {
          "type": "integer",
          "mininum": 0
        },
        "previousPageNo": {
          "type": "integer",
          "mininum": 0
        },
        "data": {
          "type": "array",
          "items": {
            "type": "object",
            "required": ["id", "name", "requestId", "cluster", "reqCreateTime" ,"reqExecTime", "reqUpdateTime", "queueStatus"],
            "additionalProperties": false,
            "properties": {
              "id": {
                "type": "integer",
                "mininum": 0
              },
              "name": {
                "type": "string"
              },
              "requestId": {
                "type": ["null","string"],
                "partern": "[0-9a-zA-Z]{24}"
              },
              "cluster": {
                "type": ["string", "null"]
              },
              "reqCreateTime": {
                "type": ["null","string"],
                "partern": "[0-9]{4}-[0-9]{2}-[0-9]{2} [0-9]{2}:[0-9]{2}:[0-9]{2}"
              },
              "reqExecTime": {
                "type": ["null","integer"],
                "mininum": 0
              },
              "reqUpdateTime": {
                "type": ["null","string"],
                "partern": "[0-9]{4}-[0-9]{2}-[0-9]{2} [0-9]{2}:[0-9]{2}:[0-9]{2}"
              },
              "queueStatus": {
                "type": "integer",
                "enum": [0, 2, 4, 6, 7, 8]
              }
            }
          }
        }
      }
    }
  }
}