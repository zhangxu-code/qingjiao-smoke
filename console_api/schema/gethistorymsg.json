{
  "$schema": "GET/api/api-tm/task/v1/getHistoryExecMsg",
  "title": "get task history msg response",
  "description": "get running history task msg response",
  "type": "object",
  "required": [
    "code",
    "data",
    "msg",
    "subCode"
  ],
  "additionalProperties": false,
  "properties": {
    "code": {
      "type": "integer",
      "enum": [
        0,
        1
      ]
    },
    "msg": {
      "type": "string"
    },
    "subCode": {
      "type": [
        "null",
        "string"
      ]
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["type", "result", "createTime"],
        "additionalProperties": false,
        "properties": {
          "type": {
            "type": "string",
            "enum": ["waiting", "resourceSuccess", "prepareStartTc", "startTcSuccess", "running", "nodeSuccess", "taskSuccess", "taskFail"]
          },
          "result": {
            "type": ["null","string"]
          },
          "createTime": {
            "type": "string",
            "partern": "[0-9]{4}-[0-9]{2}-[0-9]{2} [0-9]{2}:[0-9]{2}:[0-9]{2}"
          }
        }
      }
    }
  }
}